public class Solution {
    public int MissingNumber(int[] nums) {
        
        Array.Sort(nums);
        
        int result = 0;

        for ( int i = 0; i < nums.Length; i++)
        {
            if (nums[i] == i)
            {
                result++;
            }
            else
                
            {
                break;
            }
        }

        return result;
    }
}
